{
  "name": "vuemark",
  "version": "1.0.0",
  "private": true,
  "description": "A customizable markdown rendering library for Vue.js",
  "scripts": {
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:preview": "pnpm run -C docs preview",
    "dev": "pnpm run -C packages/debug dev --host",
    "build": "pnpm run -C packages/debug build && esno scripts/postbuild.ts"
  },
  "keywords": [
    "frontend",
    "markdown",
    "vue"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@types/node": "^20.11.16",
    "@vuemark/core": "workspace:*",
    "@vuemark/debug": "workspace:*",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "vitepress": "1.0.0-rc.35",
    "vue": "^3.4.8"
  }
}